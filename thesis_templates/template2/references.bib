@article {Ronning-CCOCARPES,
	author = {Ronning, F. and Kim, C. and Feng, D. L. and Marshall, D. S. and Loeser, A. G. and Miller, L. L. and Eckstein, J. N. and Bozovic, I. and Shen, Z.-X.},
	title = {Photoemission Evidence for a Remnant Fermi Surface and a d-Wave-Like Dispersion in Insulating Ca2CuO2Cl2},
	volume = {282},
	number = {5396},
	pages = {2067--2072},
	year = {1998},
	doi = {10.1126/science.282.5396.2067},
	publisher = {American Association for the Advancement of Science},
	abstract = {An angle-resolved photoemission study is reported on Ca2CuO2Cl2, a parent compound of high-T c superconductors. Analysis of the electron occupation probability, n(k), from the spectra shows a steep drop in spectral intensity across a contour that is close to the Fermi surface predicted by the band calculation. This analysis reveals a Fermi surface remnant, even though Ca2CuO2Cl2 is a Mott insulator. The lowest energy peak exhibits a dispersion with approximately the |coskxa {\textendash} coskya| form along this remnant Fermi surface. Together with the data from Dy-doped Bi2Sr2CaCu2O8+δ, these results suggest that this d-wave{\textendash}like dispersion of the insulator is the underlying reason for the pseudo gap in the underdoped regime.},
	issn = {0036-8075},
	URL = {https://science.sciencemag.org/content/282/5396/2067},
	eprint = {https://science.sciencemag.org/content/282/5396/2067.full.pdf},
	journal = {Science}
}

