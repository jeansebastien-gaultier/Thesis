@article{alloul-PGdiscovery,
title = {sup 89 Y NMR evidence for a fermi-liquid behavior in YBa sub 2 Cu sub 3 O sub 6+x},
author = {Alloul, H. and Ohno, T. and Mendels, P.},
abstractNote = {We report NMR shift {Delta}K and T{sub 1} data of {sup 89}Y taken from 77 to 300 K in YBa{sub 2}Cu{sub 3}O{sub 6+x} for 0.35 {lt} x {lt} 1, from the insulating to the metallic state. A Korringa law and therefore a Fermi-liqud picture is found to apply for the spin part K{sub s} of {Delta}K. The spin contribution {chi}{sub s}(x,T) to {chi}{sub m} is singled out, as the T variation of {Delta}K scales linearly with the macroscopic susceptibility {chi}{sub m}. This implies that Cu(3d) and O(2p) holes do not have independent degrees of freedom. Their hybridization, which has a {sigma} character, hardly varies with doping. These results put severe constraints on theoretical models of high-T{sub c} cuprates.},
doi = {10.1103/PhysRevLett.63.1700},
journal = {Physical Review Letters; (USA)},
issn = {0031-9007},
number = ,
volume = 63:16,
place = {United States},
year = {1989},
